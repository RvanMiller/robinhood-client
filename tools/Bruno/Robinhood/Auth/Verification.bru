meta {
  name: Verification
  type: http
  seq: 2
}

post {
  url: {{base_url}}/pathfinder/user_machine/
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "device_id": "{{device_id}}",
    "flow": "suv",
    "input": {
      "workflow_id": "{{workflow_id}}"
    }
  }
}

vars:post-response {
  machine_id: res.body.id
}

settings {
  encodeUrl: true
}
